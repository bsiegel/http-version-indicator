{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "3.0beta1",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "icons": {
    "48": "icons/icon.svg",
    "64": "icons/icon.svg",
    "96": "icons/icon.svg",
    "128": "icons/icon.svg"
  },
  "applications": {
    "gecko": {
      "id": "spdyindicator@chengsun.github.com"
    }
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "webNavigation",
    "webRequest"
  ],
  "page_action": {
    "browser_style": false
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
